generator client {
    provider = "prisma-client"
    output   = "../generated/prisma"
}

datasource db {
    provider = "postgresql"
}

model Link {
    id        Int      @id @default(autoincrement())
    longUrl   String   @map("long_url")
    short     String   @unique
    clicks    Int      @default(0)
    createdAt DateTime @default(now()) @map("created_at")

    @@map("links")
}
